@model HousingSocietyManagement.Models.Flat

<div class="card w-50">
    <div class="card-header bg-primary">
        <h3>Display the Details of : @Model.FlatName</h3>
    </div>
    <div class="card-body bg-light">
        Flat Owner : @Model.FlatOwner<br/>
        Wing   : @Model.Wing<br/>
        Aadhar  : @Model.Aadhar<br/>
        Email : @Model.Email<br/>
        Contact : @Model.Mobile<br/>
    </div>
</div>